{
	"luci-app-bnx-vpn": {
		"description": "Grant UCI access for luci-app-bnx-vpn",
		"read": {
			"uci": [ "bnx-vpn" ]
		},
		"write": {
			"uci": [ "bnx-vpn" ]
		}
	},

	"luci-access": {
		"description": "Grant access to basic LuCI procedures",
		"read": {
			"cgi-io": [ "backup", "download", "exec" ],
			"file": {
				"/etc/bnx-vpn/*": [ "read" ]
			},
			"ubus": {
				"file": [ "list", "read", "stat" ],
				"uci": [ "changes", "get" ]
			},
			"uci": [ "*" ]
		},
		"write": {
			"cgi-io": [ "upload" ],
			"file": {
				"/etc/bnx-vpn/*": [ "write" ]
			},
			"ubus": {
				"file": [ "write", "remove", "exec" ],
				"uci": [ "add", "apply", "confirm", "delete", "order", "set", "rename" ]
			},
			"uci": [ "*" ]
		}
	}

}
